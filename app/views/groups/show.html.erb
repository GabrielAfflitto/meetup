
<a href="/groups">Group Index</a>

<h1><%= @group.name %></h1>
<% @group.users.each do |user| %>
  <h3><%= user.first_name %> <%= user.last_name %></h3>

  <% user.user_groups.each do |user_group| %>
  <% next unless user_group.group_id == @group.id %>
    <%= user_group.role.first %>
  <% end %>
<% end %>
